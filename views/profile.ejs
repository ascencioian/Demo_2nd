<!doctype html>
<html>
<head>
    <title>Schedule-My-Day</title>
    <!-- css hotlink cdn for bootsrap from website -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <!-- bootstrap icon links cdn -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.6.1/font/bootstrap-icons.css">
    <!-- mapbox css link -->
    <link href='https://api.mapbox.com/mapbox-gl-js/v2.6.0/mapbox-gl.css' rel='stylesheet' />
    <!-- mapbox script -->
    <script src='https://api.mapbox.com/mapbox-gl-js/v2.6.0/mapbox-gl.js'></script>
    <!-- mabbox geocoder sript -->
    <script src="https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-geocoder/v4.7.2/mapbox-gl-geocoder.min.js"></script>
    <!-- mabbox geocoder stylesheet -->
    <link rel="stylesheet" href="https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-geocoder/v4.7.2/mapbox-gl-geocoder.css" type="text/css">
    <!-- mabbox Directions stylesheet -->
    <link rel="stylesheet" href="https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-directions/v4.1.0/mapbox-gl-directions.css" type="text/css">
    <!-- mabbox Directions script -->
    <script src="https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-directions/v4.1.0/mapbox-gl-directions.js"></script>
    <!-- stylesheet -->
    <link rel="stylesheet" href="style.css">
</head>
<body>

<!-- navbar -->
<nav class="navbar navbar-expand-lg bg-dark navbar-dark py-3 fixed-top">

  <div class="container">
      <!-- navbar logo -->
      <a href="/" class="navbar-brand">Schedule-My-Day</a>
      <!-- navbar button -->
      <button
      class="navbar-toggler"
      type="button"
      data-bs-toggle="collapse"
      data-bs-target="#navMenu"
      >
      <span class ='navbar-toggler-icon'></span> 
      </button>
      <!-- navbar links -->
      <div class="collapse navbar-collapse" id="navMenu">

          <ul class="navbar-nav ms-auto">
          <!-- <li class="nav-item">
              <a href="#learn" class="nav-link">what you will learn</a>
          </li>
          <li class="nav-item">
              <a href="#questions" class="nav-link">questions</a>
          </li>
          <li class="nav-item">
              <a href="#instructors" class="nav-link">instructors</a>
          </li> -->
          <li class="nav-item">
              <a href="/logout" class="nav-link"><span class="fa fa-user"></span>Logout</a>
          </li>
          
          <li class="nav-item">
            <a href="/feed" class="nav-link"><span class="fa fa-user"></span>HomePage</a>
          </li>
          </ul>

      </div>
  
  </div>

</nav>

    <!-- Banner -->
    <section class="bg-dark text-light p-5 p-lg-0 pt-lg-5 text-center text-sm-start">
      <div class="container">
        <h1><span class="fa fa-anchor "></span> Reference Map Lookup</h1>
        <div class ='column'>
          <div class="form-group">
        
            <input type="text" class="form form-control" name="email" placeholder="2924 Pennsylvania Avenue Lakewood, NJ 08701 " id='address'>
            <button class="btn btn-warning btn-lg mb-4 mt-1" id='geoButton'>search</button>
          </div>
        </div>
      </div>
    </section>

<div class="container">

    <div class="page-header text-center">
        
    </div>



    <div class="row">

        
        <div class="col-sm-12 col-md-6 mt-5 mb-5">
          <h2 class="text-center text-black">Bookmarks</h2>
     

            <!-- for loop that shows the signed in persons posts -->
            <% for(var i=0; i<content.length; i++) {%>
                <div class="card bg-light mb-5 ">
                  <div class="card-body text-center">
                    <p class="card-text">
                      <%= content[i].name %>
                    </p>
                    <p class="card-text">
                      <%= content[i].address %>
                    </p>
                    <span><i class="bi bi-trash-fill" aria-hidden="true"></i></span>
                  </div>
                </div>
            <% } %>
        </div>
        <div class="col-sm-12 col-md-6">
          <!-- mapbox map -->
          <section class="container mt-5 mb-5">
            <h2 class="text-center text-black">Reference Map</h2>
            <div id='map' style='width: 100%; height: 800px;'></div>
          </section>
       </div>
     
    </div>
    

         

</div>
 <!-- bootstrap javascript script -->
 <script
 src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js"
 integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4"
 crossorigin="anonymous"
></script>
<script src="main.js"></script>
<script src="geo_location.js "></script>
<script src="weather_api.js"></script>
<script src="restaurant_api.js"></script>
<script src="parks_api.js"></script>
<script src="mapbox.js"></script>
<!-- share icons -->
 <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-61a29e86c4c7ffc3"></script> 
</body>
</html>
